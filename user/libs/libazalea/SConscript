# Azalea user-mode API library.
#
# Contains parts of the Azalea API that run in user mode.

Import('env')
files = [
  "error_codes.cpp",
  "os_version.cpp",

  "processes/elf.cpp",
  "processes/exec_file.cpp",

  "syscall_user_low-x64.asm",
]

obj = [ ]
for f in files:
  obj = obj + [env.Object(f), ]

Return ("obj")